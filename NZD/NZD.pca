M = 1
N = 2
R = 3

ORG 8


IN M, 2 ; 2 ZNACI DA UNOSIMO SA FUNCKIJOM IN 2 PUTA ZA 2 ZA REDOM PROMENJIVE POCEV OD M = 1, N = 2

BGT M, N, LOOP ; AKO JE M VECE OD N NASTAVI SA LOOP, ALGORITMOM

MOV R, M ; AKO NE ZAMENIMO M I N
MOV M, N
MOV N, R

LOOP: DIV R, M, N
	
	MUL R, R, N
	SUB R, M, R

	MOV M, N
	MOV N, R

BGT R, 0, LOOP

STOP M
